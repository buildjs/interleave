let isFizzBuzz x =
  if x % 15 == 0 then
    console.log "FizzBuzz"
  else
    isBuzz x

let isBuzz x =
  if x % 5 == 0 then
    console.log "Buzz"
  else
    isFizz x

let isFizz x =
  if x % 3 == 0 then
    console.log "Fizz"
  else
